public class Atividade02

    public static Cubo{
	    private int altura;
	    private int largura;
	    private int comprimento;
	    public int calcularVolume(){
		    return (altura*largura*comprimento)
	    }
    }

    public static Paralelepipedo{
	    private int altura;
	    private int largura;
	    private int comprimento;
	    public int calcularVolume(){
		    return (altura*largura*comprimento)
	    }
    }

    public static PiramideQuadrangular{
	    private int altura;
	    private int largura;
	    private int comprimento;
	    public int calcularVolume(){
		    return (altura*(largura*comprimento))/3
	    }
    }

    public static PrismaPentagonal{
	    private int altura;
	    private int largura;
	    private int comprimento;
	    public int calcularVolume(){
		    return (altura*(largura*comprimento))
	    }
    }
